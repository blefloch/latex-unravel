            { 3 } { \@@_scan_normal_glue:  } % glue
            { 4 } { \@@_scan_mu_glue:      } % muglue
          }
          { \msg_error:nnx { unravel } { internal } { do-reg=#2 } }
      }
      { \@@_scan_int: }
    \@@_assign_register:
  }
