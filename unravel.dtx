      { \@@_do_begin_box:N #1 }
      {
        \bool_if:NTF #1
          {
            \int_case:nnTF \l_@@_head_cmd_int
              {
                { \@@_tex_use:n { hrule } } { }
                { \@@_tex_use:n { vrule } } { }
              }
              { \@@_do_leaders_rule: }
              { \@@_do_box_error: }
          }
          { \@@_do_box_error: }
      }
  }
%    \end{macrocode}
