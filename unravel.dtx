%   protected macro.
%    \begin{macrocode}
\prg_new_conditional:Npnn \@@_token_if_protected:N #1
  { p , T ,  F , TF }
  {
    \@@_token_if_expandable:NTF #1
      {
        \token_if_protected_macro:NTF #1
          { \prg_return_true: }
          {
            \token_if_protected_long_macro:NTF #1
              { \prg_return_true: }
