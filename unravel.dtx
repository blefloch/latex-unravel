                \int_gzero:N \g_@@_nonstop_int
              }
            { x } { \group_end: \@@_exit:w }
            { X } { \tex_batchmode:D \tex_end:D }
            { s } { \@@_prompt_scan_int:nn {#1} \@@_prompt_silent_steps:n }
            { o } { \@@_prompt_scan_int:nn {#1} { \int_gset:Nn \g_@@_noise_int } }
