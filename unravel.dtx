\@@_new_tex_cmd:nn { stop }                                 % 14
  {
    \@@_mode_vertical:n
      {
        \mode_if_inner:TF
          { \@@_forbidden_case: }
          {
            % ^^A todo: unless its_all_over
            \int_gdecr:N \g_@@_ends_int
            \int_compare:nNnTF \g_@@_ends_int > \c_zero
              {
                \@@_back_input:
                \@@_back_input:n
                  {
                    \@@_hbox:w to \tex_hsize:D { }
