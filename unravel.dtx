      \l_@@_head_meaning_tl \q_stop
    \exp_args:NNx \int_set:Nn \l_@@_head_char_int
      { ` \@@_token_to_char:N \l_@@_head_token }
  }
\cs_new_protected:Npn \@@_set_cmd_aux_char:w #1 ~ #2 \q_stop
  {
    \int_set:Nn \l_@@_head_cmd_int
      { \@@_tex_use:n { #1_char } }
  }
