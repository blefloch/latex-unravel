% \begin{macro}[aux]{\@@_change_if_limit:nn}
%    \begin{macrocode}
\cs_new_protected:Npn \@@_change_if_limit:nn #1#2
  {
    \int_compare:nNnTF {#2} = \g_@@_if_depth_int
